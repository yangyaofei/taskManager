#!/usr/bin/python
import socket
import parse  

sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM);  
sock.connect(('localhost', 8001));  
data = parse.parseToBinAddTask("heheheehehTest123test")
sock.send(data);  

szBuf = sock.recv(1024)
data = parse.parseTo(szBuf,1)
print "Content-type:text/html\r\n\r\n"

print(data)  
sock.close();  
print("end of coenect")
